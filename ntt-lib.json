{
	"version": "1.0.0",
	"dependencies": [
		{
			"folder": "googletest",
			"github": "https://github.com/google/googletest.git",
			"variables": {
				"CMAKE_CXX_FLAGS_DEBUG": "\"${CMAKE_CXX_FLAGS_DEBUG}\" /MDd",
				"CMAKE_CXX_FLAGS_RELEASE": "\"${CMAKE_CXX_FLAGS_RELEASE}\" /MD"
			},
			"additional": "set(gtest_force_shared_crt ON CACHE BOOL \"Use shared CRT\" FORCE)"
		},
		{
			"folder": "json",
			"github": "https://github.com/nlohmann/json.git"
		}
	]
}
